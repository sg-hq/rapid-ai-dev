{
    "name": "@photon-ai/rapid",
    "version": "1.0.1",
    "description": "Toolkit for rapid AI and agent prototyping with reusable building blocks for terminal-first development",
    "author": "GreatSomething",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/sg-hq/rapid-ai-dev.git"
    },
    "keywords": [
        "ai",
        "agent",
        "prototyping",
        "cli",
        "terminal",
        "chat",
        "tui",
        "ink",
        "typescript",
        "rapid"
    ],
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./cli-chat": {
            "types": "./dist/cli-chat/index.d.ts",
            "import": "./dist/cli-chat/index.js"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "@types/bun": "latest"
    },
    "peerDependencies": {
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "@types/react": "^19.2.2",
        "ink": "^6.4.0",
        "ink-text-input": "^6.0.0",
        "react": "^19.2.0",
        "ts-node": "^10.9.2"
    }
}
